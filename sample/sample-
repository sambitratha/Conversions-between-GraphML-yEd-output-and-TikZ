	  \node (a)[fill=red!60] at (6,8) {};  	  
	  \node (b)[fill=red!60] at (6,6) {};
	  \node (c) at (4,7) {};
 	  \node (d) at (7,7) {};
	  \node (e) at (8,7)[fill=blue!60] {};

